package learn;

import java.util.Iterator;

public class Star7{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,k,rows=5;
		for (i = 1; i <= rows; i++)
		{
		for (k = 1; k <= (2 * i - 1); k++) {
			if (k == 1 || i == rows || k == (2 * i - 1)) {
				System.out.print("*");

	}

		}
	}
}
}