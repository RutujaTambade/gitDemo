package learn;

import java.util.Iterator;

public class Star7{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i,j,rows=5;
		for (i = 1; i <= rows; i++)
		{
			for (j = i; j<=rows; j++) {
				System.out.print("");
			}
			
			System.out.print("*");

	}

		}
	}

